  },
  "homepage": "https://github.com/taratimmerman/amazon-clone-backend#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "knex": "^0.95.6",
    "pg": "^8.6.0"
  },
  "devDependencies": {
    "eslint": "^7.27.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7"
  }
